<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lo_2_grid_poster_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/lo_2_grid_poster">
        <ImageView
            android:id="@+id/lo_2_grid_poster_1_img"
            android:layout_width="@dimen/home_poster_2_grid_width"
            android:layout_height="match_parent"
            android:layout_marginEnd="@dimen/home_poster_2_grid_margin"
            android:background="@drawable/transparent_white_bg"
            android:focusable="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="ContentDescription" />
        <TextView
            android:id="@+id/lo_2_grid_poster_1_prog_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/home_poster_margin_left_right"
            android:layout_marginEnd="@dimen/home_poster_margin_left_right"
            android:layout_marginTop="@dimen/home_poster_margin_top_bottom"
            android:text="Program Time"
            android:textSize="@dimen/home_poster_txt_size"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="2"
            android:shadowRadius="2"
            android:focusable="false"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_1_img"/>
        <TextView
            android:id="@+id/lo_2_grid_poster_1_prog_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/home_poster_margin_left_right"
            android:layout_marginEnd="@dimen/home_poster_margin_left_right"
            android:layout_marginBottom="@dimen/home_poster_margin_top_bottom"
            android:text="Program Name"
            android:textSize="@dimen/home_poster_txt_size"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="2"
            android:shadowRadius="2"
            android:focusable="false"
            app:layout_constraintBottom_toBottomOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_1_img"/>
        <TextView
            android:id="@+id/lo_2_grid_poster_1_prog_channel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/home_poster_margin_left_right"
            android:layout_marginEnd="@dimen/home_poster_margin_left_right"
            android:layout_marginBottom="@dimen/home_poster_margin_top_bottom"
            android:text="Channel Name"
            android:textSize="@dimen/home_poster_txt_size"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="2"
            android:shadowRadius="2"
            android:focusable="false"
            app:layout_constraintBottom_toTopOf="@id/lo_2_grid_poster_1_prog_name"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_1_img"/>
        <View
            android:id="@+id/lo_2_grid_poster_1_frame"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/frame_focus"
            android:focusable="true"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintTop_toTopOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_1_img"/>

        <ImageView
            android:id="@+id/lo_2_grid_poster_2_img"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/home_poster_2_grid_margin"
            android:background="@drawable/transparent_white_bg"
            android:focusable="false"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/lo_2_grid_poster_1_img"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="ContentDescription" />
        <TextView
            android:id="@+id/lo_2_grid_poster_2_prog_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/home_poster_margin_left_right"
            android:layout_marginEnd="@dimen/home_poster_margin_left_right"
            android:layout_marginTop="@dimen/home_poster_margin_top_bottom"
            android:text="Program Time"
            android:textSize="@dimen/home_poster_txt_size"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="2"
            android:shadowRadius="2"
            android:focusable="false"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_2_img"/>
        <TextView
            android:id="@+id/lo_2_grid_poster_2_prog_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/home_poster_margin_left_right"
            android:layout_marginEnd="@dimen/home_poster_margin_left_right"
            android:layout_marginBottom="@dimen/home_poster_margin_top_bottom"
            android:text="Program Name"
            android:textSize="@dimen/home_poster_txt_size"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="2"
            android:shadowRadius="2"
            android:focusable="false"
            app:layout_constraintBottom_toBottomOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_2_img"/>
        <TextView
            android:id="@+id/lo_2_grid_poster_2_prog_channel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/home_poster_margin_left_right"
            android:layout_marginEnd="@dimen/home_poster_margin_left_right"
            android:layout_marginBottom="@dimen/home_poster_margin_top_bottom"
            android:text="Channel Name"
            android:textSize="@dimen/home_poster_txt_size"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowDx="1"
            android:shadowDy="2"
            android:shadowRadius="2"
            android:focusable="false"
            app:layout_constraintBottom_toTopOf="@id/lo_2_grid_poster_2_prog_name"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_2_img"/>
        <View
            android:id="@+id/lo_2_grid_poster_2_frame"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="@drawable/frame_focus"
            android:focusable="true"
            app:layout_constraintStart_toStartOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintTop_toTopOf="@id/lo_2_grid_poster_2_img"
            app:layout_constraintEnd_toEndOf="@id/lo_2_grid_poster_2_img"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>