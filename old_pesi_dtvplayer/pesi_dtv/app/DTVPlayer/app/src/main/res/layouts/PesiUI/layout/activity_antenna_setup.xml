<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:alpha="@dimen/MAIN_LAYOUT_ALPHA"
    android:background="@drawable/bg"
    tools:context="com.prime.dtvplayer.Activity.AntennaSetupActivity">

    <include layout="@layout/activity_title"
        android:id="@+id/title_layout"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/gl_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <include layout="@layout/activity_help"
        android:id="@+id/help_layout"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintTop_toTopOf="@id/gl_help"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/typeTxv"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/VIEW_HEIGHT"
        android:layout_margin="16dp"
        android:text="@string/STR_TYPE"
        android:textSize="@dimen/TEXT_SIZE"
        android:textColor="@color/text_color"
        android:background="@drawable/focus_list"
        app:layout_constraintTop_toBottomOf="@+id/title_layout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintBottom_toTopOf="@+id/gl_help"
        app:layout_constraintVertical_bias="0"/>

    <Spinner
        android:id="@+id/diseqc_type_spinner"
        android:layout_width="@dimen/ANTENNASETUP_DISEQC_TYPE_SPINNER_WIDTH"
        android:layout_height="@dimen/VIEW_HEIGHT"
        android:entries="@array/STR_DISEQC"
        android:background="@drawable/selectbox"
        android:dropDownSelector="@drawable/drop_down_selector"
        android:popupBackground="@drawable/banner"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="afterDescendants"
        app:layout_constraintTop_toTopOf="@+id/typeTxv"
        app:layout_constraintBottom_toBottomOf="@+id/typeTxv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.8"/>
    <Spinner
        android:id="@+id/page_1_to_8_spinner"
        android:layout_width="0dp"
        android:layout_height="@dimen/VIEW_HEIGHT"
        android:layout_marginTop="8dp"
        android:entries="@array/STR_PAGE"
        android:background="@drawable/selectbox"
        android:gravity="center"
        android:dropDownSelector="@drawable/drop_down_selector"
        android:popupBackground="@drawable/banner"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="afterDescendants"
        app:layout_constraintStart_toStartOf="@id/diseqc_type_spinner"
        app:layout_constraintEnd_toEndOf="@id/diseqc_type_spinner"
        app:layout_constraintTop_toBottomOf="@+id/diseqc_type_spinner"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/diseqc_off_ConstraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/gl_help"
        app:layout_constraintEnd_toEndOf="@id/diseqc_type_spinner"
        app:layout_constraintStart_toStartOf="@id/typeTxv"
        app:layout_constraintTop_toBottomOf="@+id/page_1_to_8_spinner">

        <ImageView
            android:id="@+id/antImage_diseqc_off"
            android:layout_width="@dimen/ANTENNASETUP_ANTIMAGE_SIZE"
            android:layout_height="@dimen/ANTENNASETUP_ANTIMAGE_SIZE"
            android:contentDescription="@string/STR_NULL"
            android:src="@drawable/antenna"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.5"
            app:layout_constraintHorizontal_bias="0"/>

        <ImageView
            android:id="@+id/antLine_diseqc_off_base"
            android:layout_width="0dp"
            android:layout_height="@dimen/ANTENNASETUP_ANTLINE_LENGTH"
            android:layout_marginStart="16dp"
            android:background="@android:color/white"
            android:contentDescription="@string/STR_NULL"
            app:layout_constraintEnd_toStartOf="@id/gl_spinner_diseqc_off"
            app:layout_constraintStart_toEndOf="@+id/antImage_diseqc_off"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/diseqc_off_press"
            android:layout_width="0dp"
            android:layout_height="@dimen/VIEW_HEIGHT"
            android:background="@drawable/focus_list"
            android:focusable="true"
            android:gravity="center"
            android:text="@string/STR_NONE"
            android:textColor="@color/text_color"
            android:textSize="@dimen/TEXT_SIZE"
            android:layout_marginStart="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/gl_spinner_diseqc_off"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/gl_spinner_diseqc_off"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.70" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
