<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.prime.dtvplayer.TestMiddleware.TestDVBCScanActivity">

    <Button
        android:id="@+id/buttonTestDvbcScanStartScanManual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="368dp"
        android:gravity="start|center"
        android:onClick="BtnTestStartScanManual_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/buttonTestDvbcScanStartScanAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="368dp"
        android:layout_marginTop="8dp"
        android:gravity="start|center"
        android:onClick="BtnTestStartScanAuto_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonTestDvbcScanStartScanManual" />

    <TextView
        android:id="@+id/textViewTestDvbcScanTpId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewTestDvbcScanFrequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTestDvbcScanTpId" />

    <TextView
        android:id="@+id/textViewTestDvbcScanSymbolRate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTestDvbcScanFrequency" />

    <TextView
        android:id="@+id/textViewTestDvbcScanQam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTestDvbcScanSymbolRate" />

    <TextView
        android:id="@+id/textViewTestDvbcScanLockStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/textViewTestDvbcScanFrequency"
        app:layout_constraintStart_toEndOf="@+id/textViewTestDvbcScanFrequency"
        app:layout_constraintTop_toTopOf="@+id/textViewTestDvbcScanFrequency"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewTestDvbcScanStrength"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/textViewTestDvbcScanSymbolRate"
        app:layout_constraintStart_toStartOf="@+id/textViewTestDvbcScanLockStatus"
        app:layout_constraintTop_toTopOf="@+id/textViewTestDvbcScanSymbolRate"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewTestDvbcScanQuality"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/textViewTestDvbcScanQam"
        app:layout_constraintStart_toStartOf="@+id/textViewTestDvbcScanLockStatus"
        app:layout_constraintTop_toTopOf="@+id/textViewTestDvbcScanQam"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/TestScanguildline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="481dp" />

    <TextView
        android:id="@+id/textViewTestDvbcScanTvResult"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/TestScanguildline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTestDvbcScanQam" />

    <TextView
        android:id="@+id/textViewTestDvbcScanRadioResult"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TestScanguildline"
        app:layout_constraintTop_toBottomOf="@+id/textViewTestDvbcScanQam" />

    <Button
        android:id="@+id/buttonTestDvbcScanComplete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:gravity="start|center"
        android:text="@string/STR_COMPLETE"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/buttonTestDvbcScanStartScanManual"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonTestDvbcScanCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:gravity="start|center"
        android:text="@string/STR_CANCEL"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/buttonTestDvbcScanStartScanAuto"
        app:layout_constraintTop_toBottomOf="@+id/buttonTestDvbcScanComplete" />

    <TextView
        android:id="@+id/textViewTestDvbcScanTotalTvCnt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/buttonTestDvbcScanComplete"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewTestDvbcScanTotalRadioCnt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/buttonTestDvbcScanComplete"
        app:layout_constraintTop_toBottomOf="@+id/textViewTestDvbcScanTotalTvCnt" />

    <TextView
        android:id="@+id/textViewTestDvbcScanBER"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/textViewTestDvbcScanLockStatus"
        app:layout_constraintStart_toEndOf="@+id/textViewTestDvbcScanLockStatus"
        app:layout_constraintTop_toTopOf="@+id/textViewTestDvbcScanLockStatus"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewTestDvbcScanSNR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/textViewTestDvbcScanStrength"
        app:layout_constraintStart_toStartOf="@+id/textViewTestDvbcScanBER"
        app:layout_constraintTop_toTopOf="@+id/textViewTestDvbcScanStrength"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>
