<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context="com.prime.dtvplayer.TestMiddleware.TestAvControlActivity">

    <SurfaceView
        android:id="@+id/TestsurfaceView"
        android:layout_width="360dp"
        android:layout_height="202dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toStartOf="@+id/ID_TESTAVCONTROL_GUIDELINE_RIGHT"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/ID_TESTAVCONTROL_GUIDELINE_CENTER"
        app:layout_constraintTop_toBottomOf="@+id/ID_TESTAVCONTROL_LAYOUT_TITLE" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ID_TESTAVCONTROL_GUIDELINE_RIGHT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="@dimen/SCREEN_RIGHT_EDGE" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ID_TESTAVCONTROL_GUIDELINE_LEFT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="@dimen/SCREEN_LEFT_EDGE" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ID_TESTAVCONTROL_GUIDELINE_CENTER"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ID_TESTAVCONTROL_GUIDELINE_TITLE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@dimen/TITLE_EDGE" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ID_TESTAVCONTROL_GUIDELINE_HELP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="@dimen/HELP_EDGE" />


    <include
        android:id="@+id/ID_TESTAVCONTROL_LAYOUT_TITLE"
        layout="@layout/activity_title"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        app:layout_constraintBottom_toBottomOf="@id/ID_TESTAVCONTROL_GUIDELINE_TITLE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/ID_TESTAVCONTROL_LAYOUT_HELP"
        layout="@layout/activity_help"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/ID_TESTAVCONTROL_GUIDELINE_HELP" />

