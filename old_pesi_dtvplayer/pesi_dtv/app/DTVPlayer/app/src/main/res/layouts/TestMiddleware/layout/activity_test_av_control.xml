        android:id="@+id/ID_TESTAVCONTROL_TXV_INPUT"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:scrollbars="vertical|horizontal"
        android:text=""
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/TEXT_SIZE"
        app:layout_constraintBottom_toTopOf="@+id/ID_TESTAVCONTROL_GUIDELINE_HELP"
        app:layout_constraintEnd_toStartOf="@+id/ID_TESTAVCONTROL_TXV_OUTPUT"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ID_TESTAVCONTROL_GUIDELINE_CENTER"
        app:layout_constraintTop_toBottomOf="@+id/TestsurfaceView" />

