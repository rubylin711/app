<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.prime.dtvplayer.TestMiddleware.TestProgramActivity">

    <Button
        android:id="@+id/BtnTestProgram1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start|center"
        android:onClick="BtnProgramInfoGetList_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toTopOf="@+id/guidelineTop" />

    <Button
        android:id="@+id/BtnTestProgram2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start|center"
        android:onClick="BtnProgramInfoGetSimpleChannelList_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toBottomOf="@+id/BtnTestProgram1" />

    <Button
        android:id="@+id/BtnTestProgram3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start|center"
        android:onClick="BtnProgramInfoGetByChannelId_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toBottomOf="@+id/BtnTestProgram2" />

    <Button
        android:id="@+id/BtnTestProgram4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start|center"
        android:onClick="BtnProgramInfoGetSimpleChannelByChannelId_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toBottomOf="@+id/BtnTestProgram3" />

    <Button
        android:id="@+id/BtnTestProgram5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="start|center"
        android:onClick="BtnProgramInfoSaveSimpleList_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toBottomOf="@+id/BtnTestProgram4" />

    <Button
        android:id="@+id/BtnTestProgram6"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:gravity="start|center"
        android:onClick="BtnDeleteNotReSortChnumByChId_OnClick"
        android:text="Button"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toBottomOf="@+id/BtnTestProgram5" />



    <TextView
        android:id="@+id/TvTestProgram"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="440dp"
        android:scrollbars="horizontal|vertical"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/guidelineBottom"
        app:layout_constraintEnd_toStartOf="@+id/guidelineRight"
        app:layout_constraintStart_toStartOf="@+id/guidelineLeft"
        app:layout_constraintTop_toTopOf="@+id/guidelineTop" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.02" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.98" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineTop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.02" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineBottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.98" />

</androidx.constraintlayout.widget.ConstraintLayout>
